extends layout

block content
  h1= title
  p Search results snake  
  p 
  div(class="container-fluid")
        div(class="row")
            -
                let snake = [
                    {
                        "name": "cobra",
                        "color": "black" ,
                        "weight": 30
                    },
                    {
                        "name": "busss",
                        "color": "green" ,
                        "weight": 25
                    },
                    {
                        "name": "anaconda",
                        "color": "orange" ,
                        "weight": 40
                    },                   
                ]
            each v,index in snake 
                div(class="col-sm-2" ) #{v.name}
                div(class="col-sm-2" ) #{v.color}
                div(class="col-sm-2" ) #{v.weight}

  